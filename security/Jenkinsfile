pipeline{
    agent any
    tools {
        maven '3.6.6'
        jdk 'jdk-20'
    }
    stages{
        stage('Build') {
           steps {
            sh 'mvn --version'
            sh 'java --version'
            sh 'mvn install'
           }
        }
        stage("Sonar quality check"){

            steps{
                script{
                    withSonarQubeEnv(credentialsId: 'sonar-token') {
                        sh 'cd security && mvn "-Dsonar.qualitygate=true" sonar:sonar'
                    }
                }
            }
        }
    }
}